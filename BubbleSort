def bubbleSort(ar):
   n = len(arr)
   # Traverse through all array elements
   for i in range(n):
   # Last i elements are already in correct position
   for j in range(0, n-i-1):
      
      if ar[j] > ar[j+1] :
         ar[j], ar[j+1] = ar[j+1], ar[j]

ar = ['s','h','i','b','u','i','n','t']
bubbleSort(ar)
print ("Sorted array is:")
for i in range(len(ar)):
   print (ar[i])
